<%include ../header-for-all%>
    <link rel="stylesheet" type="text/css" href="./css/trangchu.css">
    <style type="text/css">
    th,
    td {
        padding: 20px;
    }
    .content{
    	font-size: 8px;
    }
    </style>

<body>
    <%include ../home/navbar%>
    <div id="div_name_web">
        <div class="container">
            <img class="img-responsive img" src="./images/logo_soict.png" alt="logo_soict.png" width="50vmin">
            <h1 class="text-center">HỆ THỐNG QUẢN LÝ ĐỒ ÁN</h1>
        </div>
    </div>
    <div id="div_search_tool">
        <h4 class="text-center" style="margin: 20px;">KẾT QUẢ TÌM KIẾM CHO <span class="alert-success"><%=key%></span></h4>
    </div>
    <div class="container">
		<div class="row">
			<div class="col-md-8 table">
				<table class="table-responsive" border="1" width="100%">
					<thead>
						<tr class="success">
							<th>STT</th>
							<th>Tên đề tài</th>
							<th>Tác giả</th>
							<th>Star</th>
							<th>Nội dung</th>
							<th>Xem</th>
						</tr>
					</thead>
					<tbody>
						<%arrResult.forEach(function(data,index){%>
						<tr>
							<td><%=index+1%></td>
							<td><%=data.tendetai%></td>
							<td><%=data.uploadby%></td>
							<td><%=data.star%></td>
							<td class="content"><%=data.content%></td>
							<td><a href="/da/da/xemchitiet/<%=data.id%>">Xem chi tiết</a></td>
						</tr>
						<%})%>
					</tbody>
				</table>
			</div>	
		</div>
	</div>
</body>