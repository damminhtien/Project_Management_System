<%include ../header-for-all%>
<body>
	<%include ../home/navbar%>
	<div id="div_name_web">
        <div class="container">
                <img class="img-responsive img" src="./images/logo_soict.png" alt="logo_soict.png" width="50vmin">
            <h1 class="text-center">HỆ THỐNG QUẢN LÝ ĐỒ ÁN</h1>
        </div>
    </div>
    <div class="container">
		<div class="row">
			<div class="col-md-12 table">
				<table class="table-responsive table-striped" border="1" width="100%">
					<thead>
						<tr>
							<th>STT</th>
							<th>Sinh viên</th>
							<th>Nguyện vọng vào giảng viên</th>
							<th>Bộ môn</th>
							<th>Ghi chú</th>
							<th>Thời gian</th>
							<th>Loại đồ án</th>
							<th>Xác nhận</th>
						</tr>
					</thead>
					<tbody>
						<%nguyenvong.forEach(function(data,index){%>
						<tr>
							<td><%=index+1%></td>
							<td><%=data.bysinhvien%></td>
							<td><%=data.giangvien%></td>
							<td><%=data.bomon%></td>
							<td><%=data.ghichu%></td>
							<td><%=data.thoigian%></td>
							<td><%=data.da%></td>
							<td>
								<form method="post" accept-charset="utf-8" enctype="application/x-www-form-urlencoded">
									<button class="btn btn-success" type="submit" name="id" value="<%=data.id%>">Xác nhận</button>
								</form>
							</td>
						</tr>
						<%})%>
					</tbody>
				</table>
			</div>	
		</div>
	</div>
</body>